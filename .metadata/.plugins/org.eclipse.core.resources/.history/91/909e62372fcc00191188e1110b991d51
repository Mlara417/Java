package weightedAverageCalculator;
import java.util.Scanner;

public class WeightedAverage {

	public static void main(String[] args) {
		Scanner myObj = new Scanner(System.in);
		int firstSetNumber[] = new int[6];
		int firstSetMultiplier[] = new int[6];
		
		for (int i = 1; i < 7; i++) {
			System.out.println("Enter number for pair " + i + ":");
			firstSetNumber[i] = myObj.nextInt();
			System.out.println(firstSetNumber[i]);
			
			System.out.println("Enter multiplier for pair " + i + ":");
			firstSetMultiplier[i] = myObj.nextInt();
			System.out.println(firstSetMultiplier[i]);
		}
		
		System.out.println(firstSetMultiplier[6]);
		
		
		
	}

}
